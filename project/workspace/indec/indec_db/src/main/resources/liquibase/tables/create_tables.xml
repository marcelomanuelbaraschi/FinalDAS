<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
		xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

	<!--   TABLE CREATION CHANGE-SETS  -->
	<!--Tecnologia-->
	<changeSet id="Create table tecnologia" author="DAS-Team" runAlways="true" runOnChange="true">
		<preConditions onFail="MARK_RAN">
			<not>
				<tableExists tableName="tecnologia"/>
			</not>
		</preConditions>
		<sqlFile path="/src/main/resources/liquibase/tables/tecnologia.sql"/>
	</changeSet>

	<!--Tecnologia_Param-->
	<changeSet id="Create table tecnologia_param" author="DAS-Team" runAlways="true" runOnChange="true">
		<preConditions onFail="MARK_RAN">
			<not>
				<tableExists tableName="tecnologia_param"/>
			</not>
		</preConditions>
		<sqlFile path="/src/main/resources/liquibase/tables/tecnologia_param.sql"/>
	</changeSet>

	<!--Cadena-->
	<changeSet id="Create table cadena" author="DAS-Team" runAlways="true" runOnChange="true">
		<preConditions onFail="MARK_RAN">
			<not>
				<tableExists tableName="cadena"/>
			</not>
		</preConditions>
		<sqlFile path="/src/main/resources/liquibase/tables/cadena.sql"/>
	</changeSet>

	<!--Cadena_Tecnologia-->
	<changeSet id="Create table cadena_tecnologia" author="DAS-Team" runAlways="true" runOnChange="true">
		<preConditions onFail="MARK_RAN">
			<not>
				<tableExists tableName="cadena_tecnologia"/>
			</not>
		</preConditions>
		<sqlFile path="/src/main/resources/liquibase/tables/cadena_tecnologia.sql"/>
	</changeSet>

	<!--Cadena_Params_Config-->
	<changeSet id="Create table Cadena_Params_Config" author="DAS-Team" runAlways="true" runOnChange="true">
		<preConditions onFail="MARK_RAN">
			<not>
				<tableExists tableName="cadena_params_config"/>
			</not>
		</preConditions>
		<sqlFile path="/src/main/resources/liquibase/tables/cadena_params_config.sql"/>
	</changeSet>





</databaseChangeLog>