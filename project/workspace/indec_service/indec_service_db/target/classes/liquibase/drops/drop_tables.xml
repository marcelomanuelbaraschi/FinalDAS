<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
		xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">


	<!--   TABLE DROP CHANGE-SETS      -->

	<changeSet id="Drop table producto" author="DAS-Team" runAlways="true" runOnChange="true">
		<preConditions onFail="MARK_RAN">
			<tableExists tableName="producto"/>
		</preConditions>
		<dropTable tableName="producto"/>
	</changeSet>

	<changeSet id="Drop table marcaProducto" author="DAS-Team" runAlways="true" runOnChange="true">
		<preConditions onFail="MARK_RAN">
			<tableExists tableName="marcaProducto"/>
		</preConditions>
		<dropTable tableName="marcaProducto"/>
	</changeSet>

	<changeSet id="Drop table categoriaProducto" author="DAS-Team" runAlways="true" runOnChange="true">
		<preConditions onFail="MARK_RAN">
			<tableExists tableName="categoriaProducto"/>
		</preConditions>
		<dropTable tableName="categoriaProducto"/>
	</changeSet>


	<changeSet id="Drop table cadenaServiceConfig" author="DAS-Team" runAlways="true" runOnChange="true">
		<preConditions onFail="MARK_RAN">
			<tableExists tableName="cadenaServiceConfig"/>
		</preConditions>
		<dropTable tableName="cadenaServiceConfig"/>
	</changeSet>

	<changeSet id="Drop table cadena" author="DAS-Team" runAlways="true" runOnChange="true">
		<preConditions onFail="MARK_RAN">
			<tableExists tableName="cadena"/>
		</preConditions>
		<dropTable tableName="cadena"/>
	</changeSet>

	<changeSet id="Drop table tecnologia" author="DAS-Team" runAlways="true" runOnChange="true">
		<preConditions onFail="MARK_RAN">
			<tableExists tableName="tecnologia"/>
		</preConditions>
		<dropTable tableName="tecnologia"/>
	</changeSet>


</databaseChangeLog>
